udio%2F371%2Findex=' \n *\n */\n\nods noproctitle;\n\nproc format;\n\tvalue _nmissprint low-high=\"Non-missing\";\n\tvalue $_cmissprint \" \"=\" \" other=\"Non-missing\";\nrun;\n\nproc freq data=WORK.IMPORT;\n\ttitle3 \"Missing Data Frequencies\";\n\ttitle4 h=2 \"Legend: ., A, B, etc = Missing\";\n\tformat SepalLengthCm SepalWidthCm PetalLengthCm PetalWidthCm _nmissprint.;\n\tformat Species $_cmissprint.;\n\ttables Species SepalLengthCm SepalWidthCm PetalLengthCm PetalWidthCm / missing \n\t\tnocum;\nrun;\n\nproc freq data=WORK.IMPORT noprint;\n\ttable Species * SepalLengthCm * SepalWidthCm * PetalLengthCm * PetalWidthCm / \n\t\tmissing out=Work._MissingData_;\n\tformat SepalLengthCm SepalWidthCm PetalLengthCm PetalWidthCm _nmissprint.;\n\tformat Species $_cmissprint.;\nrun;\n\nproc print data=Work._MissingData_ noobs label;\n\ttitle3 \"Missing Data Patterns across Variables\";\n\ttitle4 h=2 \"Legend: ., A, B, etc = Missing\";\n\tformat SepalLengthCm SepalWidthCm PetalLengthCm PetalWidthCm _nmissprint.;\n\tformat Species $_cmissprint.;\n\tlabel count=\"Frequency\" percent=\"Percent\";\nrun;\n\ntitle3;\n\n/* Clean up */\nproc delete data=Work._MissingData_;\nrun;","ctm":"./resources/sas/models/DescribeMissingData.xml","serializedData":"<Task schemaVersion=\"5.3\">\n\n    <Registration>\n        <Name>Describe Missing Data</Name>\n        <Description>Calculate the frequency and pattern of missing values for the selected variables</Description>\n        <GUID>764b33cc-5640-4525-be1b-cb14beda00fe</GUID>\n        <Procedures>FREQ</Procedures>\n        <Links>\n            <Link href=\"http://documentation.sas.com/?softwareId=STUDIOMID&amp;softwareVersion=3.71&amp;softwareContextId=tasks&amp;requestor=inapp\">SAS Studio Task Reference Guide</Link>\n            <Link href=\"\"> </Link>\n            <Link href=\"\"> </Link>\n\t\t\t<Link href=\"http://documentation.sas.com/?softwareId=MVADOC&amp;softwareVersion=3.71_9.4&amp;softwareContextId=proc_freq&amp;requestor=inapp\">The FREQ Procedure</Link>\n            <Link href=\"\"> </Link>\n            <Link href=\"\"> </Link>\n            <Link href=\"http://support.sas.com/training/studio\">SAS Tutorials</Link>\n        </Links>\n        <Version>3.71</Version>\n    </Registration>\n\n    <Metadata>\n        <DataSources>\n            <DataSource name=\"dataset\" where=\"true\">\n            <Roles>\n            <!-- n-way tables not allowed for more than 50 variables in PROC FREQ -->\n                <Role exclude=\"freqVariable,byVariables\" maxVars=\"50\" minVars=\"1\" name=\"analysisVariables\" order=\"true\" type=\"A\">Analysis variables:</Role>\n                <Role exclude=\"analysisVariables,byVariables\" maxVars=\"1\" minVars=\"0\" name=\"freqVariable\" type=\"N\">Frequency count:</Role>\n                <Role exclude=\"analysisVariables,freqVariable\" maxVars=\"0\" minVars=\"0\" name=\"byVariables\" order=\"true\" type=\"A\">Group analysis by:</Role>\n            </Roles>\n            </DataSource>\n        </DataSources>\n\n        <Options>\n            <Option inputType=\"string\" name=\"dataTab\">DATA</Option> \n            <Option inputType=\"string\" name=\"optionsTab\">OPTIONS</Option> \n            <Option inputType=\"string\" name=\"dataGroup\">DATA</Option> \n            <Option inputType=\"string\" name=\"rolesGroup\">ROLES</Option> \n            <Option inputType=\"string\" name=\"otherRolesGroup\">ADDITIONAL ROLES</Option> \n\n<!-- Strings used in Velocity template -->\n            <Option inputType=\"string\" name=\"titleSingleVarFreq\">Missing Data Frequencies</Option>\n            <Option inputType=\"string\" name=\"titleMissingDataPattern\">Missing Data Patterns across Variables</Option>\n            <Option inputType=\"string\" name=\"titleLegend\">Legend: ., A, B, etc = Missing</Option>\n            <Option inputType=\"string\" name=\"titleFrequency\">Frequency</Option>\n            <Option inputType=\"string\" name=\"titlePercent\">Percent</Option>\n       